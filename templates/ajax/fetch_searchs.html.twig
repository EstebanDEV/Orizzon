{% for result in results %}
<a href="{{ path('profile', {'username': result.username }) }}" style="text-decoration:none;" class="box-search d-flex border-bottom p-3 justify-content-between align-items-end">
    <div class="d-flex">
        {% if result.image != "" %}
        <div style="width:50px;height:50px;">
            <img src="/uploads/profiles/{{result.image}}" class="rounded" style="width:100%;height:100%;" />
        </div>
        {% else %}
        <div style="width:50px;height:50px;">
            <img src="{{ asset('images/feed/user.png') }}" class="rounded" style="width:100%;" />
        </div>
        {% endif %}
        <div class="pl-2">
            <div style="font-size:16px;"><strong>{{ result.name }}</strong></div>
            <div>@{{ result.username }}</div>
        </div>
    </div>
</a>
{% endfor %}